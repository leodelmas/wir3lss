<form method="post" action="{{ path('ldap_user.delete', {'cn': ldapUser.cn}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ldapUser.cn) }}">
    <button class="btn">{{' Delete'|trans }}</button>
</form>
